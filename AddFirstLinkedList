#include <iostream>

using namespace std;
class Node
{
public:
    int data;
    Node* next;
};
class LinkedList
{
public:
    Node* head= nullptr;
    Node* tail= nullptr;

    void add(int x)
    {
        Node* n=new Node;
        n->data=x;
        n->next=nullptr;

        if(head==nullptr)
        {
            head=n;
            tail=n;

        }
        else
        {
            tail->next = n;
            tail= n;

        }
    }
    void print()
    {
        Node* i=head ;
        while (i!=nullptr)
        {
            cout<<i->data<<" " ;
            i=i->next;
        }
        cout << endl;
    }
  

    void AddFirst(int x)
    {
        Node* temp =new Node;
        temp->data=x;
        temp->next=nullptr;
        if (head==nullptr)
        {
            head=temp;
            tail=temp;
            return;
        }
        else
        {
            temp->next=head;
            head=temp;
        }

    }

};
int main()
{
    LinkedList l;
    l.add(2);
    l.add(3);
    l.add(9);


    l.print();
  
    l. AddFirst(1);
    l.print();



    return 0;



};

