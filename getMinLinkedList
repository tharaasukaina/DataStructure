#include <iostream>

using namespace std;
class Node
{
public:
    int data;
    Node* next;
};
class LinkedList
{
public:
    Node* head= nullptr;
    Node* tail= nullptr;

    void add(int x)
    {
        Node* n=new Node;
        n->data=x;
        n->next=nullptr;

        if(head==nullptr)
        {
            head=n;
            tail=n;

        }
        else
        {
            tail->next = n;
            tail= n;

        }
    }
    void print()
    {
        Node* i=head ;
        while (i!=nullptr)
        {
            cout<<i->data<<" " ;
            i=i->next;
        }
        cout << endl;
    }
   
     int getMin(){
       int min =head->data;
       Node* i =head ;
       while(i!=nullptr){
           if(i->data<min){
               min=i->data;
           }
           else {
               i=i->next;
           }
       }
       cout<<min;
         
         
       }
};
int main()
{
    LinkedList l;
    l.add(10);
    l.add(3);
    l.add(9);
    l.print();
    l.getMin();
    return 0;
};

